<div id="uploadContainer">

    <h1>Add Your Photos</h1>

    <%= render 'form' %>

    <%= link_to 'Back', show_path %>

</div>




<%= form_for @user, :html => {:multipart => true} do |f| %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.file_field :avatar %>
  </p>
  <p><%= f.submit %></p>
<% end %>

<div class="footer">
    <ul>
        <li><%= link_to "Logout", logout_path,  method: "delete" %></li>        
    </ul>
</div>

