<p id="notice"><%= notice %></p>
    <%= link_to 'New Photo', new_photo_path %>

<h1>Listing Photos</h1>

<div class="row">
    <% @photos.each do |p| %>
        <%= image_tag p.avatar_url, class:"col-xs-6 col-md-3" %>
        <%=  p.user.username %>
            <br />
        <button type="button" onClick="clickME()">Like</button>
            <br />
        <a id="clicks">0</a>
    <% end %>
</div>

<script>
    var clicks = 0;
    function clickME() {
        clicks += 1;
        document.getElementById("clicks").innerHTML = clicks;
    }
</script>