<p id="notice"><%= notice %></p>
    <%= link_to 'New Photo', new_photo_path %>
<%= render 'form' %>
<h1>Listing Photos</h1>

<div class="row">
    <% @photos.each do |p| %>
    <div  class="col-xs-6 col-md-3">
    
        <%= image_tag p.avatar_url , style: "max-width:300px;"%>
      <br>
        <%=  p.user.username %>
    
    <%=  p.id %>
        <%= link_to "Like Photo", vote_path(id: p.id), remote: :true %>
        <%= content_tag :h5, p.vote, id: "photo-#{p.id}" %>

    </div>
      
<!--
            <br />
        <button type="button" onClick="clickME()">Like</button>
            <br />
        <a id="clicks">0</a>
-->
  
    <% end %>
</div>

<script>
    var clicks = 0;
    function clickME() {
        clicks += 1;
        document.getElementById("clicks").innerHTML = clicks;
    }
</script>